{
  "name": "n8n-nodes-metricool-or",
  "version": "1.3.0",
  "description": "n8n node to integrate with Metricool social media management platform",
  "keywords": [
    "n8n-community-node-package",
    "metricool",
    "social-media",
    "marketing"
  ],
  "license": "MIT",
  "homepage": "https://github.com/kukis2107/n8n-nodes-metricool-or",
  "author": {
    "name": "kukis2107"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kukis2107/n8n-nodes-metricool-or.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "dist/nodes/Metricool/Metricool.node.js",
  "scripts": {
    "build": "tsc && cp nodes/Metricool/*.{svg,png} dist/nodes/Metricool/",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "!dist/**/__tests__",
    "!dist/**/*.test.*"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MetricoolApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Metricool/Metricool.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.8.2",
    "n8n-workflow": "*"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}